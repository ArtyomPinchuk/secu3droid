<?xml version="1.0" encoding="utf-8"?>

<resources>

    <attr name="title" format="string" />
    <attr name="units" format="string" />


    <declare-styleable name="FloatParamView">
        <attr name="title" />
        <attr name="units" />
        <attr name="float_value" format="float" />
        <attr name="value_precision" format="enum">
            <enum name="one" value="0"/>
            <enum name="two" value="1"/>
            <enum name="three" value="2"/>
            <enum name="four" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="IntParamView">
        <attr name="title" />
        <attr name="units" />
        <attr name="int_value" format="integer" />
    </declare-styleable>


</resources>